{
  "manifest_version": 2,
  "name": "ArtistFinder",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["js/jquery-3.2.1.min.js"]
  }
  ],
  "browser_action": {
  	"default_icon": "icon.png",
  	"default_title": "Spotify Artist Finder",
  	"default_popup": "popup.html"
  },
  "icons" : { "128" : "icon.png"}
}